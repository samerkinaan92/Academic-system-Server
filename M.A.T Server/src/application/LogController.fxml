<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LogController">
   <children>
      <TextArea fx:id="logTxtArea" editable="false" layoutX="13.0" layoutY="48.0" prefHeight="305.0" prefWidth="376.0" />
      <Label layoutX="14.0" layoutY="31.0" text="Server log">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="exitBtn" layoutX="183.0" layoutY="361.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
      <Label fx:id="serverIp" layoutX="13.0" layoutY="6.0" text="Server ip: " />
      <Label fx:id="portLbl" layoutX="218.0" layoutY="6.0" text="Port:" />
   </children>
</AnchorPane>
